<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">


    <item android:state_pressed="true">

        <shape>

            <solid android:color="#6cdd20">

            </solid>
            <stroke android:width="1dp"
                android:color="#3d850e">

            </stroke>
            <corners android:radius="@dimen/min">

            </corners>
        </shape>


    </item>

    <item>
        <shape>

            <gradient android:startColor="#766399"
                android:endColor="#55A2DF"
                android:angle="270"></gradient>
            <stroke android:width="1dp"
                android:color="#2278C7"></stroke>
            <corners android:radius="@dimen/min"></corners>

        </shape>
    </item>

</selector>